{
  "name": "cucumber-nightwatch-examples",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "nightwatch",
    "bstack-parallel": "npx nightwatch -c src/resources/conf/nightwatch-browserstack.conf.js --env browserstack.chrome --parallel ",
    "bstack-parallel-multiple": "npx nightwatch -c src/resources/conf/nightwatch-browserstack.conf.js --env browserstack.firefox,browserstack.chrome --parallel ",
    "single": "npx nightwatch -c src/resources/conf/nightwatch-browserstack.conf.js --env browserstack.chrome --tags @single",
    "single-tag": "npx nightwatch -c src/resources/conf/nightwatch-browserstack.conf.js --env browserstack.chrome --tags @single",
    "default": "npx nightwatch",
    "local": "node src/resources/conf/local.js -c src/resources/conf/nightwatch-browserstack.conf.js --env browserstack.local"

  },
  "author": "Princeton Baretto",
  "license": "MIT",
  "devDependencies": {
    "@cucumber/cucumber": "^8.5.2",
    "chromedriver": "^105.0.0",
    "nightwatch": "^2.3.3"
  },
  "dependencies": {
    "browserstack-local": "^1.5.1"
  }
}
