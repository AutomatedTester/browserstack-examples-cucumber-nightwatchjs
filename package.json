{
  "name": "cucumber-nightwatch-examples",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "on-prem-single": "npx nightwatch -c src/resources/conf/nightwatch-browserstack.conf.js --env default --parallel --tags 'not @local'",
    "on-prem": "npx nightwatch -c src/resources/conf/nightwatch-browserstack.conf.js --env default --tags @single",
    "on-prem-suite": "npx nightwatch -c src/resources/conf/nightwatch-browserstack.conf.js --tags 'not @local'",
    "bstack-parallel": "npx nightwatch -c src/resources/conf/nightwatch-browserstack.conf.js --env browserstack.chrome --parallel --tags 'not @local'",
    "bstack-parallel-multiple": "npx nightwatch -c src/resources/conf/nightwatch-browserstack.conf.js -e browserstack.chrome,browserstack.firefox,browserstack.edge,browserstack.chrome-mobile --tags 'not @local'",
    "bstack-single": "npx nightwatch -c src/resources/conf/nightwatch-browserstack.conf.js --env browserstack.chrome --tags @single",
    "bstack-local": "node src/resources/conf/local.js -c src/resources/conf/nightwatch-browserstack.conf.js --env browserstack.local --tags @local"
  },
  "author": "Princeton Baretto",
  "license": "MIT",
  "devDependencies": {
    "chromedriver": "^105.0.1",
    "geckodriver": "^3.0.2"
  },
  "dependencies": {
    "browserstack-local": "^1.5.1",
    "@cucumber/cucumber": "^8.5.2",
    "nightwatch": "^2.3.3"
  }
}
